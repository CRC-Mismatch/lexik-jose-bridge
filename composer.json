{
    "name": "spomky-labs/lexik-jose-bridge",
    "type": "symfony-bundle",
    "description": "Bridge to allow the use of web-token/jwt-framework with the Lexik JWT Authentication Bundle",
    "keywords": ["Jose", "Bundle", "Symfony"],
    "homepage": "https://github.com/Spomky-Labs/lexik-jose-bridge",
    "license": "MIT",
    "authors": [
        {
          "name": "Florent Morselli",
          "homepage": "https://github.com/Spomky"
        },
        {
          "name": "All contributors",
          "homepage": "https://github.com/Spomky-Labs/lexik-jose-bridge/contributors"
        }
    ],
    "require": {
        "web-token/jwt-bundle": "^2.0.3",
        "web-token/jwt-checker": "^2.0.3",
        "web-token/jwt-signature": "^2.0.3",
        "web-token/jwt-encryption": "^2.0.3",
        "web-token/jwt-key-mgmt": "^2.0.3",
        "lexik/jwt-authentication-bundle": "^2.0",
        "thecodingmachine/safe": "^0.1.15",
        "psr/event-dispatcher": "^1.0"
    },
    "require-dev": {
        "infection/infection": "^0.13",
        "symfony/templating": "^4.3",
        "symfony/yaml": "^4.3",
        "symfony/form": "^4.3",
        "symfony/finder": "^4.3",
        "symfony/twig-bundle": "^4.3",
        "symfony/expression-language": "^4.3",
        "symfony/dependency-injection": "^4.3",
        "symfony/var-dumper": "^4.3",
        "sensio/framework-extra-bundle": "^4.0|^5.0",
        "behat/behat": "^3.0",
        "behat/mink-extension": "^2.1",
        "behat/mink-browserkit-driver": "^1.1",
        "behat/symfony2-extension":  "^2.1",
        "phpstan/phpstan": "^0.11",
        "phpstan/phpstan-deprecation-rules": "^0.11",
        "phpstan/phpstan-strict-rules": "^0.11",
        "thecodingmachine/phpstan-safe-rule": "^0.1",
        "web-token/jwt-signature-algorithm-rsa": "^2.0.3",
        "web-token/jwt-encryption-algorithm-aesgcmkw": "^2.0.3",
        "web-token/jwt-encryption-algorithm-aesgcm": "^2.0.3",
        "web-token/jwt-signature-algorithm-hmac": "^2.0.3"
    },
    "autoload": {
        "psr-4": {
            "SpomkyLabs\\LexikJoseBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SpomkyLabs\\TestBundle\\": "Tests/Bundle/TestBundle",
            "SpomkyLabs\\LexikJoseBundle\\Features\\Context\\": "Tests/Context"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
